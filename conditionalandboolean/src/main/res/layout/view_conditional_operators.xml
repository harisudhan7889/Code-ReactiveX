<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <Button
                android:id="@+id/all"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="All"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.024" />

        <Button
                android:id="@+id/amb"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Amb"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/all"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/contains"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Contains"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/amb"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/defaultIfEmpty"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Default If Empty"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/contains"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/sequenceEqual"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Sequence Equal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/defaultIfEmpty"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/skipUntil"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Skip Until"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/sequenceEqual"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/takeUntil"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Take Until"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/skipUntil"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/skipWhile"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Skip While"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/takeUntil"
                app:layout_constraintVertical_bias="0.039" />

        <Button
                android:id="@+id/takeWhile"
                android:layout_width="359dp"
                android:layout_height="71dp"
                android:text="Take While"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.557"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/skipWhile"
                app:layout_constraintVertical_bias="0.039" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>